<!DOCTYPE html>
<html lang="ja">

<head>
  <%- include("./_share/metadata.ejs") %>
  <title></title>
  <%- include("./_share/stylesheets.ejs") %>
</head>

<body>
  <main role="main" class="container">
    <h1 class="display-1 text-center mt-5 mb-5"><i class="fa fa-fw fa-key"></i> search</h1>
    <form action="/search" method="GET" class="mt-5 mb-5">
      <div class="form-group row justify-content-center">
        <label for="staticEmail" class="col-sm-2 col-form-label">User</label>
        <div class="input-group col-sm-8">
          <input type="text" class="form-control" id="key" name="key" value="<%= locals.key %>" autofocus />
          <div class="input-group-append">
            <button class="btn btn-secondary" type="submit"><i class="fa fa-fw fa-search"></i></button>
          </div>
        </div>
      </div>
      <div class="form-group row justify-content-center">
        <label for="staticEmail" class="col-sm-2 col-form-label">Password</label>
        <div class="col-sm-8">
          <input type="text" class="form-control" id="inputPassword" value="<%= JSON.stringify(locals.data) %>" disabled>
        </div>
      </div>
    </form>
  </main>
  <%- include("./_share/javascripts.ejs") %>
</body>

</html>